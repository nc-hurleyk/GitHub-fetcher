{
  "env": {
    "browser": true,
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "no-bitwise": 2,
    "camelcase": 2,
    "curly": 2,
    "eqeqeq": [2, "allow-null"],
    "strict": [
      2,
      "global"
    ],
    "wrap-iife": [
      2,
      "any"
    ],
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "no-use-before-define": 2,
    "max-len": [
      2,
      {
        "code": 100,
        "ignoreComments": true
      }
    ],
    "no-multi-str": 2,
    "new-cap": 2,
    "no-caller": 2,
    "no-empty": 2,
    "no-irregular-whitespace": 2,
    "no-new": 2,
    "quotes": [
      2,
      "single",
      {
        "allowTemplateLiterals": true
      }
    ],
    "no-undef": 2,
    "no-unused-vars": 2,
    "semi": 2,
    "no-var": 2,
    "keyword-spacing": 2,
    "space-before-blocks": 2,
    "eol-last": 2,
    "no-duplicate-imports": 2
  },
  "globals": {
    "process": true,
    "SnapEngage": true
  }
}
